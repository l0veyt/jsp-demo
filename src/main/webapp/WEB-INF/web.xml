<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>LastVisitTimeServlet</servlet-name>
    <servlet-class>com.xinlee.demo.LastVisitTimeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>BrowsingHistoryServlet</servlet-name>
    <servlet-class>com.xinlee.demo.BrowsingHistoryServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>browsingHistoryRemoveServlet</servlet-name>
    <servlet-class>com.xinlee.demo.BrowsingHistoryRemoveServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ShoppingCartServlet</servlet-name>
    <servlet-class>com.xinlee.demo.ShoppingCartServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>IntrospectorServlet</servlet-name>
    <servlet-class>com.xinlee.demo.IntrospectorServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>BeanUtilsServlet</servlet-name>
    <servlet-class>com.xinlee.demo.BeanUtilsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LastVisitTimeServlet</servlet-name>
    <url-pattern>/lastVisitTimeServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>BrowsingHistoryServlet</servlet-name>
    <url-pattern>/browsingHistoryServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>browsingHistoryRemoveServlet</servlet-name>
    <url-pattern>/browsingHistoryRemoveServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ShoppingCartServlet</servlet-name>
    <url-pattern>/shoppingCartServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>IntrospectorServlet</servlet-name>
    <url-pattern>/introspectorServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>BeanUtilsServlet</servlet-name>
    <url-pattern>/beanUtilsServlet</url-pattern>
  </servlet-mapping>
</web-app>
